package com.asmin.array;

// using for:Each Loop

public class Array5 {

	public static void main(String[] args) {
		
		int[] arr = {10,20,30,40,50};
		for(int ele: arr) {
			System.out.println(ele);
		}
	}
}

/*  

   Syntax= for:each loop
  
   for(arrayDataType variable: arrayRefvariable){
   System.out.println(variable);
   }
  
  
  */

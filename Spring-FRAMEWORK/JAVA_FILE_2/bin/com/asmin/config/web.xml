<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="com.asmin.beans.Adddress" id="addr1" autowire-candidate="false">
		<property name="houseno" value="123" />
		<property name="city" value="BIHAR" />
		<property name="pincode" value="123736" />
	</bean>
	
	<bean class="com.asmin.beans.Adddress" id="addr2" >
		<property name="houseno" value="1234" />
		<property name="city" value="ODISHA" />
		<property name="pincode" value="123748" />
	</bean>
	
	<bean class="com.asmin.beans.Student" id="std" autowire="byType" >
		<property name="roll" value="12" />
		<property name="name" value="TINA" />
		
	</bean>

</beans>